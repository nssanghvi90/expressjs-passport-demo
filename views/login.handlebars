<h1>Login</h1>

<div>
	<form id="login" method="POST" action="/login">
		<div style="margin:20px">
			Username : <input type="text" id="username" name="username"/>
		</div>
		<div style="margin:20px">
			Password : <input type="text" id="password" name="password"/>
		</div>
		<div style="margin:20px">
			<button type="submit">Login</button>
		</div>
	</form>
</div>